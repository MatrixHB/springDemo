<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.BusDataMapper">
<!--namespace:指定为接口的全类名-->
    <select id="selectBusByNum" resultType="entities.BusData">
         select * from busdata where busnumber = #{num}
    </select>
</mapper>